  <footer>
    <ul class="row-full-width">
      <li class="footer-link-wrapper">
        <a class="footer-link mozilla-logo-wrapper" href="https://www.mozilla.org" {{>analytics/outbound-link eventLabel="Mozilla.org"}} target="_blank" aria-label="Open Mozilla.org in new window">
          {{> svg/Mozilla }}
        </a>
      </li>
      {{#each (getFooterLinks)}}
      <li class="footer-link-wrapper">
        <a class="footer-link" href="{{ this.href }}" {{> analytics/outbound-link eventLabel=this.title }} {{#if openNewWindow}} target="_blank" {{/if}}>
          {{ this.stringId }}
        </a>
      </li>
      {{/each}}
    </ul>
  </footer>
