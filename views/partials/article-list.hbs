<section class="bg-white container">
  <section class="row">
    <div class="col-12">
      <h2 class="headline">{{getString "take-control"}}</h2>
      <h3 class="subhead">{{getString "cant-stop-hackers"}}</h3>
      <ul class="article-list">
        {{#each (articleLinks)}}
          <li class="logo-title-wrapper flx">
            <div class="bullet"></div>
            <a href="" class="article-link">{{ this.title }}<span class="arrow">&#8594;</span></a>
          </li>
        {{/each}}
        <li>
        <a href="" class="more-articles">{{getString "see-more-articles"}}<span class="arrow">&#8594;</span></a>
        </li>
      </ul>
    </div>
  </section>
</section>
